WORKDIR = `pwd`

CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = -Wall
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

INC_BIN = $(INC)
CFLAGS_BIN = $(CFLAGS) -O3
RESINC_BIN = $(RESINC)
RCFLAGS_BIN = $(RCFLAGS)
LIBDIR_BIN = $(LIBDIR)
LIB_BIN = $(LIB)
LDFLAGS_BIN = $(LDFLAGS) -s
OBJDIR_BIN = obj
DEP_BIN = 
OUT_BIN = bin/RetroTMX

OBJ_BIN = aura.o main.o tmx.o

all: bin

clean: clean_bin

after_bin: 

bin: $(OUT_BIN) after_bin

$(OUT_BIN):  $(OBJ_BIN) $(DEP_BIN)
	mkdir -p bin
	$(LD) $(LIBDIR_BIN) -o $(OUT_BIN) $(OBJ_BIN)  $(LDFLAGS_BIN) $(LIB_BIN)

aura.o: aura.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c aura.c -o aura.o

main.o: main.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c main.c -o main.o

tmx.o: tmx.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c tmx.c -o tmx.o

clean_bin: 
	rm -f $(OBJ_BIN) $(OUT_BIN)
	rm -rf bin

.PHONY: after_bin clean_bin

