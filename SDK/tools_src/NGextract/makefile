WORKDIR = `pwd`

CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = 
RESINC = 
LIBDIR = 
LIB = -lSDL
LDFLAGS = -O3 -s

INC_CONSOLE = $(INC)
CFLAGS_CONSOLE = $(CFLAGS) -O3 -s
RESINC_CONSOLE = $(RESINC)
RCFLAGS_CONSOLE = $(RCFLAGS)
LIBDIR_CONSOLE = $(LIBDIR)
LIB_CONSOLE = $(LIB)
LDFLAGS_CONSOLE = $(LDFLAGS)
OBJDIR_CONSOLE = obj
DEP_CONSOLE = 
OUT_CONSOLE = bin/neogeoextract

OBJ_CONSOLE = main.o

all: console

clean: clean_console

after_console: 

console: $(OUT_CONSOLE) after_console

$(OUT_CONSOLE):  $(OBJ_CONSOLE) $(DEP_CONSOLE)
	mkdir -p bin
	$(LD) $(LIBDIR_CONSOLE) -o $(OUT_CONSOLE) $(OBJ_CONSOLE)  $(LDFLAGS_CONSOLE) $(LIB_CONSOLE)

main.o: main.c
	$(CC) $(CFLAGS_CONSOLE) $(INC_CONSOLE) -c main.c -o main.o

clean_console: 
	rm -f $(OBJ_CONSOLE) $(OUT_CONSOLE)
	rm -rf bin
	rm -rf $(OBJDIR_CONSOLE)

.PHONY: after_console clean_console

